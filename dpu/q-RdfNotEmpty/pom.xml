	<groupId>cz.cuni.mff.xrg.uv</groupId>
	<artifactId>q-RdfNotEmpty</artifactId>
	<version>1.0.0</version>
	<packaging>bundle</packaging>
	<name>q-RdfNotEmpty</name>
	<description>Fail if there are no data in given rdf data unit. Use together with run_after edge to force check before given dpus.</description>
	
	<properties>    
		<dpu.package>cz.cuni.mff.xrg.uv.transformer.check.rdfnotemtpy</dpu.package>
		<dpu.mainClass>RdfNotEmpty</dpu.mainClass>
	</properties>
	
	<dependencies>
        <dependency>
            <groupId>cz.cuni.mff.xrg.uv</groupId>
            <artifactId>boost-dpu</artifactId>
            <version>[${uk.version.min},${uk.version.max})</version>
            <scope>compile</scope>
        </dependency>
        <!-- compile library -->
        <dependency>
            <groupId>cz.cuni.mff.xrg.uv</groupId>
            <artifactId>utils-dataunit-rdf</artifactId>
            <version>[${uk.version.min},${uk.version.max})</version>
            <type>jar</type>
            <scope>compile</scope>
        </dependency>
        <dependency>
            <groupId>cz.cuni.mff.xrg.uv</groupId>
            <artifactId>service-serialization-rdf</artifactId>
            <version>[${uk.version.min},${uk.version.max})</version>
            <type>jar</type>
            <scope>compile</scope>
        </dependency>
        <dependency>
            <groupId>cz.cuni.mff.xrg.uv</groupId>
            <artifactId>service-serialization-xml</artifactId>
            <version>[${uk.version.min},${uk.version.max})</version>
            <scope>compile</scope>
        </dependency>
	</dependencies>	
	
</project>
